<nav class="navbar ">
    <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">

            <a class="navbar-brand " href="#page-top"><img class="img-responsive" src="icons/IF_Logo_home.png"></a>
        </div>


        <ul class="nav navbar-nav navbar-right">


            <li>
                <a class="btn blue" href="#team">Tour Video<i class="fa fa-caret-right fa-lg"></i></a>
            </li>
            <li>
                <a class="btn" href="#team">Help<i class="fa fa-question fa-lg"></i></a>
            </li>
        </ul>
    </div>


</nav>

<main class="main-first-page">

    <div class="container">
        <h2 id="welcome">Welcome!</h2>
        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#home">LOG IN</a></li>
            <li><a data-toggle="tab" href="#menu1">SIGN UP</a></li>

        </ul>

        <div class="tab-content">
            <div id="home" class="tab-pane fade in active">
                <h2>Log in to your account</h2>

                <form>
                    <div class="form-group">
                        <label for="email">E-mail</label>
                        <input type="email" class="form-control center-block" id="email" placeholder="Email">
                    </div>
                    <div class="form-group">
                        <label for="password">Password</label>
                        <input type="password" class="form-control center-block" id="password"
                               placeholder="Password">
                    </div>

                    <div class="form-group center-block">
                        <div class="check center-block">
                            <input type='checkbox' name='thing' value='valuable' id="thing"/><label for="thing">
                                <div id="remember">Remember</div>
                            </label>
                        </div>
                    </div>

                    <div class="form-group center-block">
                        <div class="check center-block">
                        <button type="button" class="btn login">Login</button>
  </div>
                        </div>
                    <a id="forgot-password">Forgot your password?</a>

                    <h2 id="or">OR</h2>

                    <div class="form-group center-block">
                        <div class="check center-block">
                    <a class="btn btn-block btn-social btn-facebook">
                        <i class="fa fa-facebook"></i> Log in with Facebook
                    </a>
                    </div>
                        </div>

                    <div class="form-group center-block">
                        <div class="check center-block">
                            <a class="btn btn-block btn-social btn-google">
                                <i class="fa fa-google"></i> Log in with Gmail
                            </a>
                        </div>
                    </div>

                </form>
            </div>

        </div>
    </div>


</main>


